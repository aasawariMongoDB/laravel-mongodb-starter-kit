{
    "name": "aasawari.sahasrabuddhe/mongodb-starter-kit",
    "description": "This package would be used to create Laravel applications with MongoDB",
    "type": "library",
    "require": {
      "php": "^8.1",
      "ext-mongodb": "^1.21|^2",
      "mongodb/mongodb": "^1.21|^2",
      "mongodb/laravel-mongodb": "^5.4"
    },
    "require-dev": {
      "illuminate/support": "^10.0 || ^11.0"
    },
    "autoload": {
      "psr-4": {
        "AasawariSahasrabuddhe\\MongodbStarterKit\\": "src/"
      },
      "files": [
        "src/helpers.php"
      ]
    },
    "extra": {
      "laravel": {
        "providers": [
          "AasawariSahasrabuddhe\\MongodbStarterKit\\MongoDBServiceProvider"
        ]
      }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
      {
        "name": "Aasawari Sahasrabuddhe"
      }
    ]
  }
  